var repos = [];

<% repos.each do |repo| %>
	var repo = new App.models.repo(<%= raw repo.to_json(:methods => [:tags]) %>);
	repos.push(repo);
<% end %>

App.mainRepoCollection.reset(repos, {silent : true});
App.mainReposView.render();

// User Tags shown on the right side panel.
App.mainRepoCollection.user.tags.reset(<%= raw @tags.to_json %>);
new App.views.repoTagsView({ el: "#user_tags", collection : App.mainRepoCollection.user.tags });